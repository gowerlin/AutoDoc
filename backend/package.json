{
  "name": "autodoc-agent-backend",
  "version": "1.0.0",
  "description": "AutoDoc Agent - AI-powered User Manual Generator",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test tests/e2e",
    "package": "npm run build && npm run package:all",
    "package:win": "pkg . --targets node18-win-x64 --output ../desktop/backend-bundle/backend-win.exe",
    "package:mac-intel": "pkg . --targets node18-macos-x64 --output ../desktop/backend-bundle/backend-macos-intel",
    "package:mac-arm": "pkg . --targets node18-macos-arm64 --output ../desktop/backend-bundle/backend-macos-arm",
    "package:linux": "pkg . --targets node18-linux-x64 --output ../desktop/backend-bundle/backend-linux",
    "package:all": "npm run package:win && npm run package:mac-intel && npm run package:mac-arm && npm run package:linux"
  },
  "keywords": [
    "autodoc",
    "ai",
    "documentation",
    "chrome-devtools",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.28.0",
    "express": "^4.18.0",
    "ws": "^8.14.0",
    "puppeteer-core": "^21.0.0",
    "chrome-remote-interface": "^0.33.0",
    "googleapis": "^126.0.0",
    "sharp": "^0.33.0",
    "pixelmatch": "^5.3.0",
    "html-differ": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "pg": "^8.11.0",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.3.0",
    "winston": "^3.11.0",
    "eventemitter3": "^5.0.1",
    "zod": "^3.22.0",
    "archiver": "^6.0.0",
    "diff": "^5.1.0",
    "yaml": "^2.3.4",
    "cors": "^2.8.5",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "typescript": "^5.2.0",
    "@types/node": "^20.8.0",
    "@types/express": "^4.17.0",
    "@types/ws": "^8.5.0",
    "@types/pg": "^8.10.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.0",
    "@types/archiver": "^6.0.0",
    "@types/diff": "^5.0.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "vitest": "^0.34.0",
    "@playwright/test": "^1.39.0",
    "pkg": "^5.8.1"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "node_modules/**/*"
    ],
    "targets": [
      "node18-win-x64",
      "node18-macos-x64",
      "node18-macos-arm64",
      "node18-linux-x64"
    ],
    "outputPath": "../desktop/backend-bundle"
  }
}
